<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="90"
                android:centerColor="@color/white"
                android:endColor="@color/white"
                android:startColor="#FFD7E2"
                android:type="linear" />
            <size
                android:width="24dp"
                android:height="24dp" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>
