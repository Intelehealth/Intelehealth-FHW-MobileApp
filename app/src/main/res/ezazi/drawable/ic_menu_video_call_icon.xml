<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorIconButtonBg" />
            <corners android:radius="@dimen/button_corner_radius" />
            <size
                android:width="@dimen/back_button_size"
                android:height="@dimen/back_button_size" />
            <stroke
                android:width="@dimen/input_stroke_size"
                android:color="@color/color_icon_btn_stroke" />
            <padding
                android:bottom="@dimen/std_8dp"
                android:left="@dimen/std_4dp"
                android:right="@dimen/std_4dp"
                android:top="@dimen/std_8dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_video_call" />
</layer-list>