<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/svParentMyProfile"
    android:contentDescription="My Profile Screen Parent Layout Scroll View"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".profile.MyProfileActivity">

    <LinearLayout
        android:id="@+id/llParentLayoutMyProfile"
        android:contentDescription="My Profile Screen Parent LinearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingEnd="@dimen/layout_profile_padding">

        <include
            android:id="@+id/toolbarMyProfile"
            layout="@layout/toolbar_common_ui2" />

        <LinearLayout
            android:id="@+id/llChildLayoutMyProfile"
            android:contentDescription="My Profile Screen Child LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical"
            android:paddingStart="@dimen/layout_profile_padding"
            android:paddingTop="@dimen/layout_profile_padding"
            android:paddingBottom="@dimen/layout_profile_padding">

            <androidx.cardview.widget.CardView
                android:id="@+id/cvProfileImageMyProfile"
                android:contentDescription="My Profile Screen Profile Image CardView"
                android:layout_width="@dimen/iv_profile_drawer_width"
                android:layout_height="@dimen/iv_profile_drawer_height"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/margin_16dp"
                android:elevation="0dp"
                app:cardCornerRadius="@dimen/iv_profile_photo_corner_radius">

                <ImageView
                    android:id="@+id/ivProfileImageMyProfile"
                    android:contentDescription="My Profile Screen Profile Image ImageView"
                    android:layout_width="@dimen/iv_profile_drawer_width"
                    android:layout_height="@dimen/iv_profile_drawer_height"
                    android:src="@drawable/avatar1" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/tvChangePhotoMyProfile"
                android:contentDescription="My Profile Screen Change Photo TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/achievements_user_score_margin_top"
                android:gravity="center_horizontal"
                android:lineSpacingExtra="@dimen/achievements_user_score_line_Spacing"
                android:text="@string/change_photo"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <RelativeLayout
                android:id="@+id/rlChangePassMyProfile"
                android:contentDescription="My Profile Screen Change Password RelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/tv_pass_margin_start"
                android:layout_marginTop="@dimen/tv_pass_margin_top"
                android:layout_marginEnd="@dimen/tv_pass_margin_end">

                <ImageView
                    android:id="@+id/ivChangePassIconMyProfile"
                    android:contentDescription="My Profile Screen Change Password Icon ImageView"
                    android:layout_width="@dimen/iv_camera_width"
                    android:layout_height="@dimen/iv_camera_height"
                    android:src="@drawable/ui2_ic_icons8_forgot_password" />

                <TextView
                    android:id="@+id/tvChangePassTitleMyProfile"
                    android:contentDescription="My Profile Screen Change Password Title TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/tv_change_pass_margin_start"
                    android:layout_marginTop="@dimen/tv_change_pass_margin_top"
                    android:layout_toEndOf="@+id/ivChangePassIconMyProfile"
                    android:lineSpacingExtra="@dimen/achievements_user_score_line_Spacing"
                    android:text="@string/change_password"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/common_info_text_size"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/ivChangePassArrowMyProfile"
                    android:contentDescription="My Profile Screen Change Password Arrow ImageView"
                    android:layout_width="@dimen/iv_camera_width"
                    android:layout_height="@dimen/iv_camera_height"
                    android:layout_alignParentEnd="true"
                    android:background="?attr/selectableItemBackground"
                    android:src="@drawable/ui2_ic_chevron_right" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlFingerprintLockMyProfile"
                android:contentDescription="My Profile Screen FingerPrint Lock RelativeLayout"                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/tv_pass_margin_start"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="@dimen/tv_pass_margin_end">

                <ImageView
                    android:id="@+id/ivFingerprintLockIconMyProfile"
                    android:contentDescription="My Profile Screen FingerPrint Lock Icon ImageView"
                    android:layout_width="@dimen/iv_camera_width"
                    android:layout_height="@dimen/iv_camera_height"
                    android:src="@drawable/ic_baseline_fingerprint_24" />

                <TextView
                    android:id="@+id/tvFingerprintLockTitleMyProfile"
                    android:contentDescription="My Profile Screen FingerPrint Lock Title TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/tv_change_pass_margin_start"
                    android:layout_marginTop="@dimen/tv_change_pass_margin_top"
                    android:layout_toEndOf="@+id/ivFingerprintLockIconMyProfile"
                    android:lineSpacingExtra="@dimen/achievements_user_score_line_Spacing"
                    android:text="@string/fingerprint_lock"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/common_info_text_size"
                    android:textStyle="bold" />

                <Switch
                    android:id="@+id/switchFingerprintLockMyProfile"
                    android:contentDescription="My Profile Screen FingerPrint Lock Switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:background="?attr/selectableItemBackground"/>

            </RelativeLayout>


            <TextView
                android:id="@+id/tvUsernameHeadingMyProfile"
                android:contentDescription="My Profile Screen Username Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_username_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/username"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tilUsernameMyProfile"
                android:contentDescription="My Profile Screen Username TextInputLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                app:hintEnabled="false">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/etUsernameMyProfile"
                    android:contentDescription="My Profile Screen Username EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_input_fieldnew"
                    android:enabled="false"
                    android:hint="@string/username"
                    android:imeOptions="actionNext"
                    android:paddingStart="@dimen/common_edittext_padding_left"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingEnd="@dimen/common_edittext_padding_right"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:singleLine="true"
                    android:textSize="@dimen/common_hint_text_size"
                    android:textStyle="bold" />
            </com.google.android.material.textfield.TextInputLayout>


            <TextView
                android:id="@+id/tvFirstNameHeadingMyProfile"
                android:contentDescription="My Profile Screen First Name Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/first_name"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tilFirstNameMyProfile"
                android:contentDescription="My Profile Screen First Name TextInputLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                app:hintEnabled="false">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/etFirstNameMyProfile"
                    android:contentDescription="My Profile Screen First Name EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_input_fieldnew"
                    android:hint="@string/enter_first_name"
                    android:enabled="false"
                    android:imeOptions="actionNext"
                    android:paddingStart="@dimen/common_edittext_padding_left"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingEnd="@dimen/common_edittext_padding_right"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:singleLine="true"
                    android:textSize="@dimen/common_hint_text_size" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:id="@+id/tvFirstNameErrorMyProfile"
                android:contentDescription="My Profile Screen First Name Error TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginTop="@dimen/hamburger_margin_start"
                android:lineSpacingExtra="0sp"
                android:text="@string/enter_first_name"
                android:textAppearance="@style/registr_error_msg"
                android:textColor="@color/red"
                android:textSize="@null"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tvMiddleNameHeadingMyProfile"
                android:contentDescription="My Profile Screen Middle Name Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/middle_name"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tilMiddleNameMyProfile"
                android:contentDescription="My Profile Screen Middle Name TextInputLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                app:hintEnabled="false">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/etMiddleNameMyProfile"
                    android:contentDescription="My Profile Screen Middle Name Edittext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_input_fieldnew"
                    android:hint="@string/enter_middle_name"
                    android:enabled="false"
                    android:imeOptions="actionNext"
                    android:paddingStart="@dimen/common_edittext_padding_left"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingEnd="@dimen/common_edittext_padding_right"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:singleLine="true"
                    android:textSize="@dimen/common_hint_text_size" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:id="@+id/tvLastNameHeadingMyProfile"
                android:contentDescription="My Profile Screen Last Name Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/last_name"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tilLastNameMyProfile"
                android:contentDescription="My Profile Screen Last Name TextInputLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                app:hintEnabled="false">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/etLastNameMyProfile"
                    android:contentDescription="My Profile Screen Last Name Edittext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_input_fieldnew"
                    android:hint="@string/enter_last_name"
                    android:imeOptions="actionNext"
                    android:paddingStart="@dimen/common_edittext_padding_left"
                    android:enabled="false"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingEnd="@dimen/common_edittext_padding_right"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:singleLine="true"
                    android:textSize="@dimen/common_hint_text_size" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:id="@+id/tvLastNameErrorMyProfile"
                android:contentDescription="My Profile Screen Last Name Error TextView"                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginTop="@dimen/hamburger_margin_start"
                android:lineSpacingExtra="0sp"
                android:text="@string/enter_last_name"
                android:textAppearance="@style/registr_error_msg"
                android:textColor="@color/red"
                android:textSize="@null"
                android:visibility="gone"
                tools:visibility="visible"/>

            <TextView
                android:id="@+id/tvGenderHeadingMyProfile"
                android:contentDescription="My Profile Screen Gender Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/gender"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <RadioGroup
                android:id="@+id/rgGenderMyProfile"
                android:contentDescription="My Profile Screen Gender RadioGroup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                android:orientation="horizontal"
                android:weightSum="3">

                <RadioButton
                    android:id="@+id/rbMaleMyProfile"
                    android:contentDescription="My Profile Screen Male RadioButton"                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:paddingStart="@dimen/space_5dp"
                    android:enabled="false"
                    android:layout_weight="1"
                    android:paddingEnd="@dimen/space_5dp"
                    android:drawableRight="@drawable/gender_male_svg"
                    android:text="@string/male"
                    android:textSize="@dimen/select_language"
                    android:theme="@style/RadioButtonStyleSelection" />

                <RadioButton
                    android:id="@+id/rbFemaleMyProfile"
                    android:contentDescription="My Profile Screen Female RadioButton"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="15dp"
                    android:paddingStart="@dimen/space_5dp"
                    android:enabled="false"
                    android:paddingEnd="@dimen/space_5dp"
                    android:layout_weight="1"
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/gender_female_svg"
                    android:text="@string/female"
                    android:textSize="@dimen/select_language"
                    android:theme="@style/RadioButtonStyleSelection" />

                <RadioButton
                    android:id="@+id/rbOtherMyProfile"
                    android:contentDescription="My Profile Screen Other RadioButton"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="end"
                    android:layout_weight="1"
                    android:enabled="false"
                    android:paddingStart="@dimen/space_5dp"
                    android:paddingEnd="@dimen/space_5dp"
                    android:drawableRight="@drawable/gender_other_svg"
                    android:text="@string/other"
                    android:textSize="@dimen/select_language"
                    android:theme="@style/RadioButtonStyleSelection" />
            </RadioGroup>

            <LinearLayout
                android:id="@+id/llDOBAgeHeadingLayoutMyProfile"
                android:contentDescription="My Profile Screen DOB Age Heading LinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:orientation="horizontal"
                android:weightSum="1">

                <TextView
                    android:id="@+id/tvDOBHeadingMyProfile"
                    android:contentDescription="My Profile Screen DOB Heading TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.6"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                    android:text="@string/date_of_birth"
                    android:textColor="@color/textColorGray"
                    android:textSize="@dimen/common_label_text_size" />

                <TextView
                    android:id="@+id/tvAgeHeadingMyProfile"
                    android:contentDescription="My Profile Screen Age Heading TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dob_age_margin_start"
                    android:layout_weight="0.4"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                    android:text="@string/age"
                    android:textColor="@color/textColorGray"
                    android:textSize="@dimen/common_label_text_size" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llDOBAgeLayoutMyProfile"
                android:contentDescription="My Profile Screen DOB Age LinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                android:orientation="horizontal"
                android:weightSum="1">

                <TextView
                    android:id="@+id/tvDOBMyProfile"
                    android:contentDescription="My Profile Screen DOB TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.6"
                    android:background="@drawable/ui2_background_input_field_new"
                    android:drawableEnd="@drawable/ui2_ic_calendar"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                    android:paddingStart="@dimen/dob_date_padding_start"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingEnd="@dimen/dob_date_padding_end"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:text="May 10, 2022"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/common_info_text_size" />

                <TextView
                    android:id="@+id/tvAgeMyProfile"
                    android:contentDescription="My Profile Screen Age TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dob_age_margin_start"
                    android:layout_weight="0.4"
                    android:background="@drawable/ui2_background_input_field_new"
                    android:drawableEnd="@drawable/ui2_ic_calendar"
                    android:drawableTint="@color/transparent"
                    android:gravity="center_vertical"
                    android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                    android:paddingStart="@dimen/dob_date_padding_start"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:text="28"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/common_info_text_size" />
            </LinearLayout>

                <TextView
                    android:id="@+id/tvDOBAgeErrorMyProfile"
                    android:contentDescription="My Profile Screen DOB Age Error TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:letterSpacing="0.02"
                    android:layout_gravity="end"
                    android:textAlignment="viewEnd"
                    android:lineSpacingExtra="0sp"
                    android:text="@string/hw_min_age"
                    android:layout_marginTop="@dimen/hamburger_margin_start"
                    android:textAppearance="@style/registr_error_msg"
                    android:textColor="@color/red"
                    android:textSize="@null"
                    android:visibility="gone"
                    tools:visibility="visible"/>

            <TextView
                android:id="@+id/tvPhoneNumHeadingProfile"
                android:contentDescription="My Profile Screen Phone Num Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/phone_number"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <RelativeLayout
                android:id="@+id/rlPhoneNumLayoutProfile"
                android:contentDescription="My Profile Screen Phone Num Layout RelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.hbb20.CountryCodePicker
                    android:id="@+id/ccpMyProfile"
                    android:contentDescription="My Profile Screen Country Code Picker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvPhoneNumHeadingProfile"
                    android:layout_marginTop="@dimen/et_new_pass_margin_top"
                    android:layout_marginEnd="@dimen/spinner_countries_margin_end"
                    android:layout_weight="1"
                    android:background="@drawable/ui2_spinner_background"
                    android:gravity="center_vertical"
                    android:paddingStart="@dimen/country_code_picker_padding_start"
                    android:paddingTop="@dimen/country_code_picker_padding_top"
                    android:paddingEnd="@dimen/country_code_picker_padding_start"
                    android:paddingBottom="@dimen/country_code_picker_padding_top"
                    app:ccp_countryPreference="us,in"
                    app:ccp_textSize="@dimen/country_code_picker_text_size" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/tilPhoneNumMyProfile"
                    android:contentDescription="My Profile Screen Phone Num TextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/et_new_pass_margin_top"
                    android:layout_toEndOf="@+id/ccpMyProfile"
                    app:hintEnabled="false">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/etPhoneNumMyProfile"
                        android:contentDescription="My Profile Screen Phone Num EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bg_input_fieldnew"
                        android:hint="@string/enter_phone_number"
                        android:imeOptions="actionNext"
                        android:inputType="phone"
                        android:lineSpacingExtra="3sp"
                        android:paddingStart="@dimen/common_edittext_padding_left"
                        android:paddingTop="@dimen/common_edittext_padding_top"
                        android:paddingEnd="@dimen/common_edittext_padding_right"
                        android:paddingBottom="@dimen/common_edittext_padding_bottom"
                        android:singleLine="true"
                        android:textSize="@dimen/common_hint_text_size" />
                </com.google.android.material.textfield.TextInputLayout>
            </RelativeLayout>

            <TextView
                android:id="@+id/tvPhoneNumErrorMyProfile"
                android:contentDescription="My Profile Screen Phone Num Error TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginTop="@dimen/hamburger_margin_start"
                android:lineSpacingExtra="0sp"
                android:text="@string/enter_phone_number"
                android:textAppearance="@style/registr_error_msg"
                android:textColor="@color/red"
                android:textSize="@null"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tvEmailHeadingMyProfile"
                android:contentDescription="My Profile Screen Email Heading TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/profile_tv_label_margin_top"
                android:letterSpacing="0.02"
                android:lineSpacingExtra="@dimen/profile_text_line_spacing"
                android:text="@string/email"
                android:textColor="@color/textColorGray"
                android:textSize="@dimen/common_label_text_size" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tilEmailMyProfile"
                android:contentDescription="My Profile Screen Email TextInputLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/et_new_pass_margin_top"
                app:hintEnabled="false">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/etEmailMyProfile"
                    android:contentDescription="My Profile Screen Email EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_input_fieldnew"
                    android:hint="@string/enter_your_email"
                    android:imeOptions="actionDone"
                    android:paddingStart="@dimen/common_edittext_padding_left"
                    android:paddingTop="@dimen/common_edittext_padding_top"
                    android:paddingEnd="@dimen/common_edittext_padding_right"
                    android:paddingBottom="@dimen/common_edittext_padding_bottom"
                    android:singleLine="true"
                    android:textSize="@dimen/common_hint_text_size" />
            </com.google.android.material.textfield.TextInputLayout>

            <Button
                android:id="@+id/btnSaveMyProfile"
                android:contentDescription="My Profile Screen Save Button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_privacy_notice_link"
