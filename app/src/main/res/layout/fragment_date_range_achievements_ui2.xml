<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/svParentDRAchievements"
    android:contentDescription="Date Range Achievements Parent ScrollView"
    android:fillViewport="true">

    <LinearLayout
        android:id="@+id/llParentDRAchievements"
        android:contentDescription="Date Range Achievements Parent LinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/date_frag_parent_margin_top"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/llDateDRAchievements"
            android:contentDescription="Date Range Achievements Date LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/date_frag_layout_margin_start"
            android:layout_marginEnd="@dimen/date_frag_layout_margin_end"
            android:orientation="horizontal"
            android:weightSum="2">

            <LinearLayout
                android:id="@+id/llStartDateDRAchievements"
                android:contentDescription="Date Range Achievements Start Date LinearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvFromDRAchievements"
                    android:contentDescription="Date Range Achievements 'From' TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:letterSpacing="0.02"
                    android:text="@string/from"
                    android:textColor="@color/textColorGray"
                    android:textSize="@dimen/date_frag_layout_text_size" />

                <TextView
                    android:id="@+id/tvStartDateDRAchievements"
                    android:contentDescription="Date Range Achievements 'Start Date' TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/date_frag_date_margin_top"
                    android:background="@drawable/ui2_date_picker_bg"
                    android:gravity="center_horizontal|center_vertical"
                    android:letterSpacing="0.02"
                    android:paddingEnd="@dimen/date_frag_date_padding_end"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/date_frag_date_text_size"
                    app:drawableEndCompat="@drawable/ui2_ic_calendar"
                    app:drawableTint="@color/iconTintGray" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/llEndDateDRAchievements"
                android:contentDescription="Date Range Achievements End Date LinearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/date_frag_layout2_margin_start"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvToDRAchievements"
                    android:contentDescription="Date Range Achievements 'To' TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:letterSpacing="0.02"
                    android:text="@string/to"
                    android:textColor="@color/textColorGray"
                    android:textSize="@dimen/date_frag_layout_text_size" />

                <TextView
                    android:id="@+id/tvEndDateDRAchievements"
                    android:contentDescription="Date Range Achievements 'End Date' TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/ui2_date_picker_bg"
                    android:gravity="center_horizontal|center_vertical"
                    android:letterSpacing="0.02"
                    android:paddingEnd="20dp"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/date_frag_date_text_size"
                    app:drawableEndCompat="@drawable/ui2_ic_calendar"
                    app:drawableTint="@color/iconTintGray" />

            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:id="@+id/llChild1DRAchievements"
            android:contentDescription="Date Range Achievements Child 1 LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/card1_common_achieve_margin_start"
            android:layout_marginTop="@dimen/layout_cards_date_range_margin_top"
            android:layout_marginEnd="@dimen/card1_common_achieve_margin_end"
            android:layout_marginBottom="@dimen/card1_common_achieve_margin_bottom"
            android:orientation="horizontal"
            android:weightSum="2">

            <LinearLayout
                android:id="@+id/llPatientAddedDRAchievements"
                android:contentDescription="Date Range Achievements Patient Added LinearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/card1_achieve_margin_end"
                android:layout_weight="1"
                android:background="@drawable/ui2_patients_card_bg_1"
                android:orientation="vertical"
                android:padding="@dimen/card_common_achieve_padding">

                <ImageView
                    android:id="@+id/ivPatientAddedIconDRAchievements"
                    android:contentDescription="Date Range Achievements Patient Added Icon ImageView"
                    android:layout_width="@dimen/card_achieve_icon_width"
                    android:layout_height="@dimen/card_achieve_icon_height"
                    android:src="@drawable/ui2_icon_user_add" />

                <TextView
                    android:id="@+id/tvPatientAddedDescDRAchievements"
                    android:contentDescription="Date Range Achievements Patient Added Description TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_title_margin_top"
                    android:text="@string/patients_added"
                    android:textSize="@dimen/date_frag_layout_text_size" />

                <TextView
                    android:id="@+id/tvPatientAddedValueDRAchievements"
                    android:contentDescription="Date Range Achievements Patient Added Value TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_value_margin_top"
                    android:gravity="top"
                    android:lineHeight="@dimen/card_achieve_value_line_height"
                    android:lineSpacingExtra="@dimen/card_achieve_value_line_spacing"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/card_achieve_value_textsize"
                    android:textStyle="bold" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/llVisitCompletedDRAchievements"
                android:contentDescription="Date Range Achievements Visit Completed LinearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/card1_achieve_margin_start"
                android:layout_weight="1"
                android:background="@drawable/ui2_patients_card_bg_blue"
                android:orientation="vertical"
                android:padding="@dimen/card_common_achieve_padding">

                <ImageView
                    android:id="@+id/ivVisitCompletedIconDRAchievements"
                    android:contentDescription="Date Range Achievements Visit Completed Icon ImageView"
                    android:layout_width="@dimen/card_achieve_icon_width"
                    android:layout_height="@dimen/card_achieve_icon_height"
                    android:src="@drawable/ui2_ic_icon_visits_completed" />

                <TextView
                    android:id="@+id/tvVisitCompletedDescDRAchievements"
                    android:contentDescription="Date Range Achievements Visit Completed Description TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_title_margin_top"
                    android:text="@string/visits_completed_by_you"
                    android:textSize="@dimen/date_frag_layout_text_size" />

                <TextView
                    android:id="@+id/tvVisitCompletedValueDRAchievements"
                    android:contentDescription="Date Range Achievements Visit Completed Value TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_value_margin_top"
                    android:gravity="top"
                    android:lineHeight="@dimen/card_achieve_value_line_height"
                    android:lineSpacingExtra="@dimen/card_achieve_value_line_spacing"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/card_achieve_value_textsize"
                    android:textStyle="bold" />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/llChild2DRAchievements"
            android:contentDescription="Daily Achievements Child 2 LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/card2_common_achieve_margin_start"
            android:layout_marginEnd="@dimen/card2_common_achieve_margin_end"
            android:orientation="horizontal"
            android:weightSum="2">


            <LinearLayout
                android:id="@+id/llSatisfactionScoreDRAchievements"
                android:contentDescription="Date Range Achievements Satisfaction Score LinearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/card1_achieve_margin_end"
                android:layout_weight="1"
                android:background="@drawable/ui2_patients_card_bg_green"
                android:orientation="vertical"
                android:padding="@dimen/card_common_achieve_padding">

                <ImageView
                    android:id="@+id/ivSatisfactionScoreIconDRAchievements"
                    android:contentDescription="Date Range Achievements Satisfaction Score Icon ImageView"
                    android:layout_width="@dimen/card_achieve_icon_width"
                    android:layout_height="@dimen/card_achieve_icon_height"
                    android:src="@drawable/ui2_ic_star_score" />

                <TextView
                    android:id="@+id/tvSatisfactionScoreDescDRAchievements"
                    android:contentDescription="Date Range Achievements Satisfaction Score Description TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_title_margin_top"
                    android:text="@string/your_patient_score"
                    android:textSize="@dimen/date_frag_layout_text_size" />

                <TextView
                    android:id="@+id/tvSatisfactionScoreValueDRAchievements"
                    android:contentDescription="Date Range Achievements Satisfaction Score Value TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_value_margin_top"
                    android:gravity="top"
                    android:lineHeight="@dimen/card_achieve_value_line_height"
                    android:lineSpacingExtra="@dimen/card_achieve_value_line_spacing"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/card_achieve_value_textsize"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/llTimeSpentDRAchievements"
                android:contentDescription="Date Range Achievements Time Spent LinearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/card1_achieve_margin_start"
                android:layout_weight="1"
                android:background="@drawable/ui2_patients_card_bg_1"
                android:orientation="vertical"
                android:padding="@dimen/card_common_achieve_padding">

                <ImageView
                    android:id="@+id/ivTimeSpentIconDRAchievements"
                    android:contentDescription="Date Range Achievements Time Spent Icon ImageView"
                    android:layout_width="@dimen/card_achieve_icon_width"
                    android:layout_height="@dimen/card_achieve_icon_height"
                    android:src="@drawable/ui2_ic_time_spent" />

                <TextView
                    android:id="@+id/tvTimeSpentDescDRAchievements"
                    android:contentDescription="Date Range Achievements Time Spent Desc TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_title_margin_top"
                    android:text="@string/total_time_spent"
                    android:textSize="@dimen/date_frag_layout_text_size" />

                <TextView
                    android:id="@+id/tvTimeSpentValueDRAchievements"
                    android:contentDescription="Date Range Achievements Time Spent Value TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/card_achieve_value_margin_top"
                    android:gravity="top"
                    android:lineHeight="@dimen/card_achieve_value_line_height"
                    android:lineSpacingExtra="@dimen/card_achieve_value_line_spacing"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/card_achieve_value_textsize"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="50dp" />
    </LinearLayout>
</ScrollView>