SELECT E.visituuid,E.uuid, E.provider_uuid, E.encounter_type_uuid, U.name,
E.encounter_time FROM tbl_encounter E, tbl_uuid_dictionary U
where E.visituuid ="96892258-3ae4-4c58-839a-5077b703e61a" and E.voided = '0'
and U.uuid = E.encounter_type_uuid ORDER BY DATE(encounter_time) DESC

SELECT P.uuid, P.openmrs_id, P.first_name, P.last_name, P.middle_name, P.date_of_birth, PA.value as bedNo
FROM tbl_patient P LEFT OUTER JOIN tbl_patient_attribute PA ON PA.patientuuid = P.uuid
AND PA.person_attribute_type_uuid = (SELECT uuid FROM tbl_patient_attribute_master  WHERE name = 'Bed Number')
LEFT OUTER JOIN tbl_visit V ON V.patientuuid = P.uuid
LEFT OUTER JOIN tbl_encounter E ON E.visituuid = V.uuid
LEFT OUTER JOIN tbl_uuid_dictionary U ON U.uuid = E.encounter_type_uuid
WHERE V.creator = '90bde499-9548-4ab0-8849-8f288ad82109'
AND E.encounter_type_uuid != 'bd1fbfaa-f5fb-4ebd-b75c-564506fc309e
GROUP BY P.uuid  ORDER BY P.first_name, V.startdate, U.name ASC

SELECT COUNT(uuid) FROM tbl_visit  V WHERE uuid NOT IN (Select visituuid FROM tbl_encounter WHERE  encounter_type_uuid ='bd1fbfaa-f5fb-4ebd-b75c-564506fc309e' )

SELECT COUNT(V.uuid) FROM tbl_visit  V
INNER JOIN tbl_patient P ON P.uuid = V.patientuuid
WHERE V.uuid NOT IN (Select visituuid FROM tbl_encounter WHERE  encounter_type_uuid ='bd1fbfaa-f5fb-4ebd-b75c-564506fc309e' )

SELECT V.uuid as visitUuid, P.uuid, P.openmrs_id, P.first_name, P.last_name, P.middle_name, P.date_of_birth, PA.value as bedNo FROM tbl_visit  V
LEFT OUTER JOIN tbl_patient P ON P.uuid = V.patientuuid
LEFT OUTER JOIN tbl_patient_attribute PA ON PA.patientuuid = P.uuid
AND PA.person_attribute_type_uuid = (SELECT uuid FROM tbl_patient_attribute_master  WHERE name = 'Bed Number')
WHERE V.uuid NOT IN (Select visituuid FROM tbl_encounter WHERE  encounter_type_uuid ='bd1fbfaa-f5fb-4ebd-b75c-564506fc309e' ) AND V.voided = "0"
GROUP BY V.uuid ORDER BY P.first_name

Select COUNT() from tbl_patient where uuid IN
(SELECT patientuuid FROM tbl_visit WHERE uuid NOT IN
(Select visituuid FROM tbl_encounter WHERE  encounter_type_uuid ='bd1fbfaa-f5fb-4ebd-b75c-564506fc309e' ))

SELECT COUNT(*), V.uuid as visitUuid, P.uuid, P.openmrs_id, P.first_name, P.last_name, P.middle_name, P.date_of_birth, PA.value as bedNo FROM tbl_visit  V
LEFT OUTER JOIN tbl_patient P ON P.uuid = V.patientuuid
LEFT OUTER JOIN tbl_patient_attribute PA ON PA.patientuuid = P.uuid
AND PA.person_attribute_type_uuid = (SELECT uuid FROM tbl_patient_attribute_master  WHERE name = 'Bed Number')
WHERE V.uuid NOT IN (Select visituuid FROM tbl_encounter WHERE  encounter_type_uuid ='bd1fbfaa-f5fb-4ebd-b75c-564506fc309e' ) AND V.voided = "0"
GROUP BY V.uuid ORDER BY P.first_name