SELECT E.visituuid,E.uuid, E.provider_uuid, E.encounter_type_uuid, U.name, E.encounter_time FROM tbl_encounter E, tbl_uuid_dictionary U where E.visituuid ="96892258-3ae4-4c58-839a-5077b703e61a" and E.voided = '0'  and U.uuid = E.encounter_type_uuid ORDER BY DATE(encounter_time) DESC